<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<resource-bundle>
			<base-name>ua.kiev.doctorvera.web.resources.Message</base-name>
			<var>MESSAGE</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>ua.kiev.doctorvera.web.resources.Mapping</base-name>
			<var>MAPPINGS</var>
		</resource-bundle>

		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>

		<action-listener>org.primefaces.application.DialogActionListener</action-listener>
		<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
		<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
		


	</application>

	<managed-bean>
		<managed-bean-name>test</managed-bean-name>
		<managed-bean-class>ua.kiev.doctorvera.managedbeans.Test</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>addressView</managed-bean-name>
		<managed-bean-class>ua.kiev.doctorvera.managedbeans.AddressView</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
